{
  "name": "azwebdevs.org",
  "version": "2.0.0",
  "description": "azwebdevs.org",
  "private": true,
  "scripts": {
    "lint": "eslint . --cache",
    "postinstall": "npm prune",
    "prestart": "npm run start:mongo",
    "start": "sh scripts/start.sh",
    "stop": "npm run stop:mongo",
    "start:mongo": "mongod --config mongod.conf --fork",
    "stop:mongo": "killall -9 mongod && echo Press ENTER to continue",
    "build": "npm run build:backend && npm run build:client",
    "build:clean": "rimraf build",
    "build:common": "sh scripts/build-common.sh",
    "build:server": "sh scripts/build-server.sh",
    "build:backend": "npm run build:server && npm run build:common",
    "build:client": "sh scripts/build-client.sh",
    "build:prod": "sh scripts/build-prod.sh",
    "test": "npm run lint && ava",
    "test:watch": "ava --watch"
  },
  "author": "Kevin Lanni (https://github.com/therealklanni)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therealklanni/azwebdevs.org.git"
  },
  "bugs": {
    "url": "https://github.com/therealklanni/azwebdevs.org/issues"
  },
  "homepage": "https://github.com/therealklanni/azwebdevs.org#readme",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "chalk": "^1.1.1",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "koa": "^1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-compose": "^2.3.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "microseconds": "^0.1.0",
    "mongoose": "^4.4.3",
    "mongoose-findorcreate": "^0.1.2",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "uuid": "^2.0.1",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "eslint": "^2.0.0",
    "eslint-config-troysconfig": "^1.0.8",
    "eslint-plugin-react": "^4.0.0",
    "faucet": "0.0.1",
    "foreman": "^1.4.1",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.14.1"
  }
}
